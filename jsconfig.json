{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*",
      ],
      "@navigation/*": [
        "src/navigation/*",
      ],
      "@screens/*": [
        "src/screens/*",
      ],
      "@styles/*": [
        "src/styles/*",
      ],
      "@test/*": [
        "src/test/*",
      ],
      "@utils/*": [
        "src/utils/*",
      ],
      "@assets/*": [
        "./assets/*",
      ],
    }
  }
}